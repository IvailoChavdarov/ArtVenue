@model ViewModelWithPublicationList
@section Heads{
    <link rel="stylesheet" href="~/css/Posts/index.css" asp-append-version="true" />
}
@{
    ViewData["Title"] = "Saved";
}
<div class="row">
    <div class="posts-sidenav-container col-12 col-lg-3 col-md-3">
        <div class="posts-sidenav sticky-top">
            <partial name="_PostsSideNavPartial" />
        </div>
    </div>
    <div class="col">
        <partial name="_PostsListPartial" />
        <partial name="_PostsPageSliderPartial"/>
    </div>
    <div class="col-3"></div>
</div>
@section Scripts{
    <script src="~/js/posts.js"></script>
}